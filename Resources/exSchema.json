{    "title": "Person",    "type": "object",    "properties": {        "firstName": {            "type": "string"         },        "lastName": {            "type": "string"         },        "gender": {            "description": "Gender is M or F or not tell",            "enum": [ "M","F","NT" ]        },        "birthday": {            "type": "object",            "properties": {                "day": {                    "type": "number",                    "minimum": 1,                    "maximum":31                    },                "month": {                    "type": "number",                    "minimum": 1,                    "maximum":12                    },                "year": {"type": "number"}            },            "required": ["day", "month", "year"]        }    },    "required": ["firstName", "lastName"]}